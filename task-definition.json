{
  "family": "next-13-demo-task-definition",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "next-13-demo-container",
      "image": "next-13-demo",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "MINI_API_KEY",
          "valueFrom": "/fargate-next-13-demo/mini_api_key"
        },
        {
          "name": "SCHEMA_PATH",
          "valueFrom": "/fargate-next-13-demo/schema_path"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/next-13-demo-task-definition",
          "awslogs-region": "ap-northeast-1",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "next-13-demo",
          "awslogs-datetime-format": "\\[%d/%b/%Y:%H:%M:%S %z\\]"
        }
      }
    }
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": ["FARGATE"],
  "taskRoleArn": "arn:aws:iam::092991348446:role/AmazonECSTaskParameterStoreRole",
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}
